{
  "schemaVersion": 1,
  "id": "arclojure",
  "version": "${version}",
  "name": "Arclojure",
  "description": "A Minecraft mod template combining Clojure's functional programming with Architectury's cross-platform capabilities.",
  "authors": [
    "Arclojure Team"
  ],
  "contact": {
    "homepage": "https://github.com/your-username/arclojure",
    "sources": "https://github.com/your-username/arclojure"
  },
  "license": "MIT",
  "icon": "assets/arclojure/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.arclojure.fabric.ModMainFabric"
    ],
    "client": [
      "com.arclojure.fabric.client.ModMainFabricClient"
    ]
  },
  "mixins": [
    "arclojure.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.4",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*"
  }
}
