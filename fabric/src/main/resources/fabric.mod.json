{
  "schemaVersion": 1,
  "id": "fabric-language-clojure",
  "version": "${version}",
  "name": "Fabric Language Clojure",
  "description": "Fabric language module for Clojure. Adds support for Clojure entrypoints and bundles the Clojure runtime.",
  "authors": [
    "fabriclj Team"
  ],
  "contact": {
    "homepage": "https://github.com/pumpkingod/fabric-language-clojure",
    "sources": "https://github.com/pumpkingod/fabric-language-clojure",
    "issues": "https://github.com/pumpkingod/fabric-language-clojure/issues"
  },
  "license": "MIT",
  "icon": "assets/fabric-language-clojure/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.fabriclj.fabric.ModMainFabric"
    ],
    "client": [
      "com.fabriclj.fabric.client.ModMainFabricClient"
    ]
  },
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": ">=1.20",
    "java": ">=17"
  },
  "custom": {
    "fabric-language-clojure:bundled_libraries": {
      "org.clojure:clojure": "${clojure_version}",
      "nrepl:nrepl": "${nrepl_version}"
    }
  }
}
